<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.MyPass" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:statusBarColor">@color/blue69</item>
        <item name="android:background">@color/blue69</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:navigationBarColor">@color/white</item>
        <item name="android:windowLightNavigationBar" tools:targetApi="o_mr1">true</item>

    </style>

    <style name="welcomeScreenTheme" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:statusBarColor">@color/blue69</item>
        <item name="android:background">@color/blue69</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:navigationBarColor">@color/blue69</item>

    </style>

    <style name="mainTheme" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:statusBarColor">#9F9F9F</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:navigationBarColor">@color/white</item>

    </style>

</resources>